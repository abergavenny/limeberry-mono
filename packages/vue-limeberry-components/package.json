{
  "name": "@abergavenny/vue-limeberry-components",
  "version": "0.0.1",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/styles.css": "./dist/styles.css"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "types/*.d.ts"
