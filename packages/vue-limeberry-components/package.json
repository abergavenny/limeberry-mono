  },
  "dependencies": {
    "@abergavenny/limeberry-components": "*",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.1.0",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vue-tsc": "^0.40.4"
  },
  "publishConfig": {
    "access": "public"
  }
