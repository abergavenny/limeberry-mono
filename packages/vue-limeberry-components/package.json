{
  "name": "@abergavenny/vue-limeberry-components",
  "version": "0.0.3-alpha.5",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/styles.css": "./dist/styles.css"
  },
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "types/*.d.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:dts": "vue-tsc --declaration --emitDeclarationOnly && vite build",
    "preview": "vite preview",
    "clean:build": "rimraf dist"
  },
  "devDependencies": {
    "@abergavenny/limeberry-components": "^0.0.3-alpha.5",
    "@rollup/plugin-typescript": "^8.5.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vue": "^3.2.37",
    "vue-tsc": "^0.40.13"
  },
  "publishConfig": {
    "access": "public"
  }
}
